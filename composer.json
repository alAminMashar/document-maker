{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "africastalking/africastalking": "^3.0",
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laraveldaily/laravel-charts": "^0.2.1",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "mckenziearts/laravel-notify": "^2.5",
        "romanzipp/laravel-queue-monitor": "^5.3",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.10",
        "tinymce/tinymce": "^6.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "wnx/laravel-stats": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AfriTalking\\AfriTalkingServiceProvider"
            ],
            "aliases": {
                "Sms": "AfriTalking\\Facades\\AfriTalking"
            },
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "github-oauth": {
            "github.com": "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"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
